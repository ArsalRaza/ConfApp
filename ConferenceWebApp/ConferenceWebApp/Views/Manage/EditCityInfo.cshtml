@model ConferenceWebApp.Models.FormModels.ManageModel.CityInformationModel
@{
    ViewBag.Title = "EditCityInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>City Information</h2>
@using (@Html.BeginForm())
{
    <form role="form">
        <div class="form-group">
            <label for="email">@Html.DisplayNameFor(m => m.CityInformation):</label>&nbsp;&nbsp;<label>@Html.ValidationMessageFor(m => m.CityInformation)</label>

            @Html.TextAreaFor(m => m.CityInformation, new {@class="form-control", @cols = "80", @rows = "20" })
        </div>

        <div class="form-group">
            <button type="submit" class="btn btn-default">Save City Information</button>

        </div>
    </form>
}